programa
{
    inicio
        real numero
        //     este é um número real
        caractere letra
        inteiro idade

        /*este bloco tem operações de leia
        e escreva*/
        escreva("Digite um número real: ")
        leia(numero)
        escreva("Digite uma letra: ")
        leia(letra)
        //teste
        escreva("Digite sua idade: ")
        leia(idade)

        // resultado
        escreva(numero)
        escreva(letra)
        escreva(idade)
}